                         {{#each cartItems}}
                        <tr class="table-row">
                            
                            <td class="column-1">
                                <div class="cart-img-product b-rad-4 o-f-hidden">
                                    <img src={{itemImage}} alt="IMG-PRODUCT">
                                </div>
                            </td>
                            <td class="column-2">{{itemName}}</td>
                            <td class="column-3">{{restaurantName}}</td>
                            <td class="column-4">{{itemCost}}</td>
                            <td class="column-5">
                                <div class="flex-w bo5 of-hidden w-size17">
                                    <button class="btn-num-product-down color1 flex-c-m size7 bg8 eff2">
                                        <i class="fs-12 fa fa-minus" aria-hidden="true"></i>
                                    </button>
    
                                    <input class="size8 m-text18 t-center num-product" type="number" name="num-product1" value={{itemQuantity}}>
    
                                    <button class="btn-num-product-up color1 flex-c-m size7 bg8 eff2">
                                        <i class="fs-12 fa fa-plus" aria-hidden="true"></i>
                                    </button>
                                   
                                </div>
                                <a href="#" class="remove-item"style="padding-right:20px; ">Remove Item </a>
                            </td>
                            {{!-- total cost of item --}}
                            <td class="column-6"> <span class="total-item-cost"> </span></td>
                            
                        </tr>
                         {{/each}}
    
                        
                    </table>
                </div>
